	**********************************************
	Column 6 : Frequency killers
	**********************************************
	-->

      <WidgetGroup>
        <Layout>vertical</Layout>
		<MinimumSize>30,110</MinimumSize>
		<MaximumSize>40,110</MaximumSize>
        <Children>
			
			<!--
			**********************************************
			Button- Frequency Kill
			**********************************************
			-->
			<PushButton>
				<TooltipId>filterHighKill</TooltipId>
				<MinimumSize>20,20</MinimumSize>
				<MaximumSize>20,20</MaximumSize>
				<Style></Style>
				<NumberStates>2</NumberStates>
				<State>
					<Number>0</Number>
					<Pressed>btn_kill<Variable name="i"/>_over.png</Pressed>
