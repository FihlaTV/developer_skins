				</State>
				<State>
					<Number>1</Number>
					<Pressed>btn_repeat<Variable name="i"/>_over.png</Pressed>
					<Unpressed>btn_repeat<Variable name="i"/>_over.png</Unpressed>
				</State>
				<Connection>
					<ConfigKey><Variable name="channel"/>,repeat</ConfigKey>
				</Connection>
			</PushButton>
			
			
			<!--
			**********************************************
			Button- Prelisten / Monitoring
			**********************************************
			-->
			<PushButton>
				<TooltipId>pfl</TooltipId>
				<MinimumSize>30,20</MinimumSize>
				<MaximumSize>40,30</MaximumSize>
				<Style></Style>
				<NumberStates>2</NumberStates>
				<State>
					<Number>0</Number>
					<Pressed>btn_pfl<Variable name="i"/>.png</Pressed>
					<Unpressed>btn_pfl<Variable name="i"/>.png</Unpressed>
				</State>
				<State>
					<Number>1</Number>
					<Pressed>btn_pfl<Variable name="i"/>_over.png</Pressed>
					<Unpressed>btn_pfl<Variable name="i"/>_over.png</Unpressed>
				</State>
				<Connection>
					<ConfigKey><Variable name="channel"/>,pfl</ConfigKey>
				</Connection>
			</PushButton>
			
			
		</Children>
	  </WidgetGroup>






	<!--
	**********************************************
	Column 5 : Volume
	**********************************************
	-->

      <WidgetGroup>
        <Layout>vertical</Layout>
		<MinimumSize>35,100</MinimumSize>
		<MaximumSize>40,100</MaximumSize>
        <Children>
			
			
			
			<!--
			**********************************************
			Slider- Channel Volume
			**********************************************
			-->
			<SliderComposed>
				<TooltipId>channel_volume</TooltipId>
				<MinimumSize>35,100</MinimumSize>
				<MaximumSize>40,100</MaximumSize>
				<Style></Style>
				<!--Handle>knob_volume1.png</Handle>
				<Slider>slider_volume1.png</Slider-->
				<Handle>knob_pitch<Variable name="i"/>.png</Handle>
				<Slider>slider_pitch<Variable name="i"/>.png</Slider>
				<Horizontal>false</Horizontal>
				<Connection>
					<ConfigKey><Variable name="channel"/>,volume</ConfigKey>
					<EmitOnDownPress>false</EmitOnDownPress>
				</Connection>
			</SliderComposed>
			
			
        </Children>
      </WidgetGroup>



