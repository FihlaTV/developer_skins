				</Connection>
			</PushButton>
			
			<PushButton>
				<TooltipId>filterMidKill</TooltipId>
				<MinimumSize>20,20</MinimumSize>
				<MaximumSize>20,20</MaximumSize>
				<Style></Style>
				<NumberStates>2</NumberStates>
				<State>
					<Number>0</Number>
					<Pressed>btn_kill<Variable name="i"/>_over.png</Pressed>
					<Unpressed>btn_kill<Variable name="i"/>.png</Unpressed>
				</State>
				<State>
					<Number>1</Number>
					<Pressed>btn_kill<Variable name="i"/>_over.png</Pressed>
					<Unpressed>btn_kill<Variable name="i"/>_over.png</Unpressed>
				</State>
				<Connection>
					<ConfigKey><Variable name="channel"/>,filterMidKill</ConfigKey>
					<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
