<!DOCTYPE template>
<!--
Description:
Track control panel.
-->
<Template>
	<SetVariable name="channel">[Channel<Variable name="i"/>]</SetVariable>
	
  <WidgetGroup>
    <ObjectName>Deck</ObjectName>
    <Layout>horizontal</Layout>
    <SizePolicy>me,me</SizePolicy>
    <MinimumSize>210,100</MinimumSize>
    <MaximumSize>250,110</MaximumSize>
    <Children>



	<!--
	**********************************************
	Column 1
	**********************************************
	-->

      <WidgetGroup>
		<Layout>vertical</Layout>
		<MinimumSize>35,110</MinimumSize>
		<MaximumSize>40,110</MaximumSize>
		<Children>
			
			<!--
			**********************************************
			Slider- Pitch control
			**********************************************
			-->
			<SliderComposed>
				<TooltipId>rate</TooltipId>
				<Style></Style>
				<MinimumSize>35,75</MinimumSize>
				<MaximumSize>35,75</MaximumSize>
				<Handle>knob_pitch<Variable name="i"/>.png</Handle>
				<Slider>slider_pitch<Variable name="i"/>.png</Slider>
				<Horizontal>false</Horizontal>
				<Connection>
					<ConfigKey><Variable name="channel"/>,rate</ConfigKey>
					<EmitOnDownPress>false</EmitOnDownPress>
				</Connection>
			</SliderComposed>

			
        </Children>
      </WidgetGroup>



	<!--
	**********************************************
	Column 2
	**********************************************
	-->


      <WidgetGroup>
		<Layout>vertical</Layout>
		<MinimumSize>35,40</MinimumSize>
		<MaximumSize>40,60</MaximumSize>
		<Children>
			
			<!--
			****************************************
			 Button- Play
			****************************************
			-->
			<PushButton>
				<TooltipId>play_cue_set</TooltipId>
				<MinimumSize>20,20</MinimumSize>
				<MaximumSize>40,30</MaximumSize>
				<Style></Style>
				<NumberStates>2</NumberStates>
				<RightClickIsPushButton>true</RightClickIsPushButton>
				<State>
					<Number>0</Number>
					<Pressed>btn_play<Variable name="i"/>.png</Pressed>
					<Unpressed>btn_play<Variable name="i"/>.png</Unpressed>
				</State>
				<State>
					<Number>1</Number>
					<Pressed>btn_play<Variable name="i"/>_over.png</Pressed>
					<Unpressed>btn_play<Variable name="i"/>_over.png</Unpressed>
				</State>
				<Pos>3,3</Pos>
				<Connection>
					<ConfigKey><Variable name="channel"/>,play</ConfigKey>
					<EmitOnDownPress>true</EmitOnDownPress>
					<ButtonState>LeftButton</ButtonState>
				</Connection>
				<Connection>
					<ConfigKey><Variable name="channel"/>,cue_set</ConfigKey>
					<EmitOnDownPress>true</EmitOnDownPress>
					<ButtonState>RightButton</ButtonState>
					<ConnectValueToWidget>false</ConnectValueToWidget>
				</Connection>
			</PushButton>
			
			
			
			
			<!--
			**********************************************
			Button- Beat sync
			**********************************************
			-->
			<PushButton>
				<TooltipId>beatsync_beatsync_tempo</TooltipId>
				<Style></Style>
				<NumberStates>1</NumberStates>
				<State>
					<Number>0</Number>
					<Pressed>btn_sync<Variable name="i"/>_over.png</Pressed>
					<Unpressed>btn_sync<Variable name="i"/>.png</Unpressed>
				</State>
				<Pos>41,241</Pos>
				<Connection>
					<ConfigKey><Variable name="channel"/>,beatsync</ConfigKey>
					<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
					<ButtonState>LeftButton</ButtonState>
				</Connection>
				<Connection>
					<ConfigKey><Variable name="channel"/>,beatsync_tempo</ConfigKey>
					<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
					<ButtonState>RightButton</ButtonState>
				</Connection>
			</PushButton>

			<!--
			**********************************************
			Button- BPM Tap
			**********************************************
			Little trickery here:
			BPM tap is transparent png directly over BPM display, so it became BPM and TAP at once,
			changed tooltip accordingly
			-->
			<!--PushButton>
				<TooltipId>bpm_tap_visual_bpm</TooltipId>
				<Style></Style>
				<NumberStates>1</NumberStates>
				<State>
					<Number>0</Number>
					<Pressed>btn_tap1_over.png</Pressed>
					<Unpressed>btn_tap1.png</Unpressed>
				</State>
				<Pos>307,80</Pos>
				<Connection>
					<ConfigKey><Variable name="channel"/>,bpm_tap</ConfigKey>
					<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
				</Connection>
			</PushButton-->
			
			
			<!--
			**********************************************
			Button- Key lock
			**********************************************
			-->

			<PushButton>
				<TooltipId>keylock</TooltipId>
				<MinimumSize>35,35</MinimumSize>
				<MaximumSize>35,35</MaximumSize>
				<Style></Style>
				<NumberStates>2</NumberStates>
				<State>
					<Number>0</Number>
					<Pressed>btn_keylock<Variable name="i"/>.png</Pressed>
					<Unpressed>btn_keylock<Variable name="i"/>.png</Unpressed>
				</State>
				<State>
					<Number>1</Number>
					<Pressed>btn_keylock<Variable name="i"/>_over.png</Pressed>
					<Unpressed>btn_keylock<Variable name="i"/>_over.png</Unpressed>
				</State>
				<Pos>3,265</Pos>
				<Connection>
					<ConfigKey><Variable name="channel"/>,keylock</ConfigKey>
				</Connection>
			</PushButton>
			
			
			
        </Children>
      </WidgetGroup>






	<!--
	**********************************************
	Column 3 
	**********************************************
	-->

      <WidgetGroup>
        <Layout>vertical</Layout>
		<MinimumSize>35,110</MinimumSize>
		<MaximumSize>40,110</MaximumSize>
        <Children>
			
			
			<!--
			**********************************************
			Button- Fast forward and rewind
			**********************************************
			-->
			
			<WidgetGroup>
				<Layout>horizontal</Layout>
				<MinimumSize>40,20</MinimumSize>
				<MaximumSize>60,20</MaximumSize>
				<Children>
					
					<PushButton>
						<TooltipId>back_start</TooltipId>
						<Style></Style>
						<NumberStates>1</NumberStates>
						<State>
							<Number>0</Number>
							<Pressed>btn_rewind<Variable name="i"/>_over.png</Pressed>
							<Unpressed>btn_rewind<Variable name="i"/>.png</Unpressed>
						</State>
						<Pos>3,27</Pos>
						<Connection>
							<ConfigKey><Variable name="channel"/>,back</ConfigKey>
							<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
							<ButtonState>LeftButton</ButtonState>
						</Connection>
						<Connection>
							<ConfigKey><Variable name="channel"/>,start</ConfigKey>
							<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
							<ButtonState>RightButton</ButtonState>
						</Connection>
					</PushButton>
					
					<PushButton>
						<TooltipId>fwd_end</TooltipId>
						<Style></Style>
						<NumberStates>1</NumberStates>
						<State>
							<Number>0</Number>
							<Pressed>btn_forward<Variable name="i"/>_over.png</Pressed>
							<Unpressed>btn_forward<Variable name="i"/>.png</Unpressed>
						</State>
						<Pos>22,27</Pos>
						<Connection>
							<ConfigKey><Variable name="channel"/>,fwd</ConfigKey>
							<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
							<ButtonState>LeftButton</ButtonState>
						</Connection>
						<Connection>
							<ConfigKey><Variable name="channel"/>,end</ConfigKey>
							<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
							<ButtonState>RightButton</ButtonState>
						</Connection>
					</PushButton>
				</Children>
			</WidgetGroup>
			
			<!--
			**********************************************
			Button- Hotcue
			**********************************************
			-->
			<WidgetGroup>
				<Layout>vertical</Layout>
				<MinimumSize>40,20</MinimumSize>
				<MaximumSize>60,40</MaximumSize>
				<Children>
					
					
					<WidgetGroup>
						<Layout>horizontal</Layout>
						<MinimumSize>40,20</MinimumSize>
						<MaximumSize>60,20</MaximumSize>
						<Children>
							
							<PushButton>
								<TooltipId>hotcue</TooltipId>
								<Style></Style>
								<NumberStates>2</NumberStates>
								<LeftClickIsPushButton>true</LeftClickIsPushButton>
								<RightClickIsPushButton>true</RightClickIsPushButton>
								<State>
									<Number>0</Number>
									<Pressed>btn_hotcue<Variable name="i"/>_1_over.png</Pressed>
									<Unpressed>btn_hotcue<Variable name="i"/>_1.png</Unpressed>
								</State>
								<State>
									<Number>1</Number>
									<Pressed>btn_hotcue<Variable name="i"/>_1.png</Pressed>
									<Unpressed>btn_hotcue<Variable name="i"/>_1_over.png</Unpressed>
								</State>
								<Pos>41,6</Pos>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_1_activate</ConfigKey>
									<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
									<ButtonState>LeftButton</ButtonState>
									<ConnectValueToWidget>false</ConnectValueToWidget>
								</Connection>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_1_clear</ConfigKey>
									<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
									<ButtonState>RightButton</ButtonState>
									<ConnectValueToWidget>false</ConnectValueToWidget>
								</Connection>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_1_enabled</ConfigKey>
									<ConnectValueFromWidget>false</ConnectValueFromWidget>
								</Connection>
							</PushButton>
							
							<PushButton>
								<TooltipId>hotcue</TooltipId>
								<Style></Style>
								<NumberStates>2</NumberStates>
								<LeftClickIsPushButton>true</LeftClickIsPushButton>
								<RightClickIsPushButton>true</RightClickIsPushButton>
								<State>
									<Number>0</Number>
									<Pressed>btn_hotcue<Variable name="i"/>_2_over.png</Pressed>
									<Unpressed>btn_hotcue<Variable name="i"/>_2.png</Unpressed>
								</State>
								<State>
									<Number>1</Number>
									<Pressed>btn_hotcue<Variable name="i"/>_2.png</Pressed>
									<Unpressed>btn_hotcue<Variable name="i"/>_2_over.png</Unpressed>
								</State>
								<Pos>41,29</Pos>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_2_activate</ConfigKey>
									<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
									<ButtonState>LeftButton</ButtonState>
									<ConnectValueToWidget>false</ConnectValueToWidget>
								</Connection>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_2_clear</ConfigKey>
									<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
									<ButtonState>RightButton</ButtonState>
									<ConnectValueToWidget>false</ConnectValueToWidget>
								</Connection>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_2_enabled</ConfigKey>
									<ConnectValueFromWidget>false</ConnectValueFromWidget>
								</Connection>
							</PushButton>
							
						</Children>
					</WidgetGroup>
					
					<WidgetGroup>
						<Layout>horizontal</Layout>
						<MinimumSize>40,20</MinimumSize>
						<MaximumSize>60,20</MaximumSize>
						<Children>
							
							
							<PushButton>
								<TooltipId>hotcue</TooltipId>
								<Style></Style>
								<NumberStates>2</NumberStates>
								<LeftClickIsPushButton>true</LeftClickIsPushButton>
								<RightClickIsPushButton>true</RightClickIsPushButton>
								<State>
									<Number>0</Number>
									<Pressed>btn_hotcue<Variable name="i"/>_3_over.png</Pressed>
									<Unpressed>btn_hotcue1_3.png</Unpressed>
								</State>
								<State>
									<Number>1</Number>
									<Pressed>btn_hotcue<Variable name="i"/>_3.png</Pressed>
									<Unpressed>btn_hotcue<Variable name="i"/>_3_over.png</Unpressed>
								</State>
								<Pos>60,6</Pos>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_3_activate</ConfigKey>
									<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
									<ButtonState>LeftButton</ButtonState>
									<ConnectValueToWidget>false</ConnectValueToWidget>
								</Connection>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_3_clear</ConfigKey>
									<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
									<ButtonState>RightButton</ButtonState>
									<ConnectValueToWidget>false</ConnectValueToWidget>
								</Connection>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_3_enabled</ConfigKey>
									<ConnectValueFromWidget>false</ConnectValueFromWidget>
								</Connection>
							</PushButton>
							
							<PushButton>
								<TooltipId>hotcue</TooltipId>
								<Style></Style>
								<NumberStates>2</NumberStates>
								<LeftClickIsPushButton>true</LeftClickIsPushButton>
								<RightClickIsPushButton>true</RightClickIsPushButton>
								<State>
									<Number>0</Number>
									<Pressed>btn_hotcue<Variable name="i"/>_4_over.png</Pressed>
									<Unpressed>btn_hotcue<Variable name="i"/>_4.png</Unpressed>
								</State>
								<State>
									<Number>1</Number>
									<Pressed>btn_hotcue<Variable name="i"/>_4.png</Pressed>
									<Unpressed>btn_hotcue<Variable name="i"/>_4_over.png</Unpressed>
								</State>
								<Pos>60,29</Pos>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_4_activate</ConfigKey>
									<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
									<ButtonState>LeftButton</ButtonState>
									<ConnectValueToWidget>false</ConnectValueToWidget>
								</Connection>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_4_clear</ConfigKey>
									<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
									<ButtonState>RightButton</ButtonState>
									<ConnectValueToWidget>false</ConnectValueToWidget>
								</Connection>
								<Connection>
									<ConfigKey><Variable name="channel"/>,hotcue_4_enabled</ConfigKey>
									<ConnectValueFromWidget>false</ConnectValueFromWidget>
								</Connection>
							</PushButton>
							
						</Children>
					</WidgetGroup>
					
				</Children>
			</WidgetGroup>
			
        </Children>
      </WidgetGroup>



	<!--
	**********************************************
	Column 4
	**********************************************
	-->

	  <WidgetGroup>
		<Layout>vertical</Layout>
		<MinimumSize>30,110</MinimumSize>
		<MaximumSize>40,110</MaximumSize>
		<Children>
			
			
			<!--
			**********************************************
			Button - Quantize
			**********************************************
			-->
			<PushButton>
				<TooltipId>quantize</TooltipId>
				<MinimumSize>40,20</MinimumSize>
				<MaximumSize>60,40</MaximumSize>
				<Style></Style>
				<NumberStates>2</NumberStates>
				<State>
					<Number>0</Number>
					<Pressed>btn_quantize<Variable name="i"/>.png</Pressed>
					<Unpressed>btn_quantize<Variable name="i"/>.png</Unpressed>
				</State>
				<State>
					<Number>1</Number>
					<Pressed>btn_quantize<Variable name="i"/>_over.png</Pressed>
					<Unpressed>btn_quantize<Variable name="i"/>_over.png</Unpressed>
				</State>
				<Connection>
					<ConfigKey><Variable name="channel"/>,quantize</ConfigKey>
					<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
					<ButtonState>LeftButton</ButtonState>
				</Connection>
			</PushButton>
			
			
			<!--
			**********************************************
			Button- Repeat (End of track mode)
			**********************************************
			-->
			<PushButton>
				<TooltipId>repeat</TooltipId>
				<Style></Style>
				<MinimumSize>40,20</MinimumSize>
				<MaximumSize>60,40</MaximumSize>
				<NumberStates>2</NumberStates>
				<State>
					<Number>0</Number>
