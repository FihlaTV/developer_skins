<!DOCTYPE template>
<!--
Description:
Horizontal Slider.
-->
<Template>
	<WidgetGroup>
		<Layout>horizontal</Layout>
		<SizePolicy><Variable name="policy_w"/>,<Variable name="policy_h"/></SizePolicy>
		<Children>
			
			<SliderComposed>
				<TooltipId><Variable name="tooltip_id"/></TooltipId>
				<!--Size>60,35</Size-->
				<Slider>slider-horizontal-<Variable name="slider_w"/>x<Variable name="slider_h"/>.svg</Slider>
				<Handle>handle-horizontal-<Variable name="handle_w"/>x<Variable name="handle_h"/>.svg</Handle>
				<Horizontal>true</Horizontal>
				<Connection>
					<ConfigKey><Variable name="channel"/>,<Variable name="command"/></ConfigKey>
				</Connection>
			</SliderComposed>
			
		</Children>
	</WidgetGroup>
</Template>




