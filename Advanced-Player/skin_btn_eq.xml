<!DOCTYPE template>
<!--
Description:
Equalizer slider
-->
<Template>
	
	<SliderComposed>
		
		<TooltipId>filter<Variable name="frequency"/></TooltipId>
		
		<Slider>slider-horizontal-60x<Variable name="height"/>-<Variable name="frequency"/>.svg</Slider>
		<Handle>handle-horizontal-10x<Variable name="height"/>-<Variable name="i"/>.svg</Handle>
		
		<Horizontal>true</Horizontal>
		
		<Connection>
			<ConfigKey><Variable name="channel"/>,filter<Variable name="frequency"/></ConfigKey>
		</Connection>
		
	</SliderComposed>
	
</Template>

