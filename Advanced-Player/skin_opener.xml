<!DOCTYPE template>
<!--
Description:
Opener.
-->
<Template>
	<WidgetGroup>
		<Layout>horizontal</Layout>
		<!--Style>QGroupBox { border: 1px solid red; }</Style-->
		<Children>
			
			<PushButton>
				<NumberStates>2</NumberStates>
				<State>
					<Number>0</Number>
					<Pressed>btn_forward1.png</Pressed>
					<Unpressed>btn_forward1.png</Unpressed>
				</State>
				<State>
					<Number>1</Number>
					<Pressed>btn_rewind1.png</Pressed>
					<Unpressed>btn_rewind1.png</Unpressed>
				</State>
				<Connection>
					<ConfigKey><Variable name="channel"/>,<Variable name="action"/></ConfigKey>
				</Connection>
			</PushButton>
			
		</Children>
	</WidgetGroup>
</Template>




