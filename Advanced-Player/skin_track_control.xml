<!DOCTYPE template>
<!--
Description:
Track control panel.
-->
<Template>
	<SetVariable name="channel">[Channel<Variable name="i"/>]</SetVariable>
	
	
	<WidgetGroup>
	<ObjectName>Deck</ObjectName>
	<Layout>horizontal</Layout>
	<SizePolicy>min,me</SizePolicy>
	<Style>
		/** /
		QGroupBox { border: 1px solid red; }
		/**/
	</Style>
	<Children>


	<!--Template src="skin:skin_spacer_vx.xml">
		<SetVariable name="height">7</SetVariable>
	</Template-->
	
	<!--
	**********************************************
	Column 1
	**********************************************
	-->
	<WidgetGroup>
		<ObjectName>VolumeColumn</ObjectName>
		<Layout>vertical</Layout>
		<SizePolicy>min,min</SizePolicy>
		<MinimumSize>25,-1</MinimumSize>
		<MaximumSize>33,-1</MaximumSize>
		<Children>
			
			<Template src="skin:skin_spacer_vx.xml">
				<SetVariable name="height">14</SetVariable>
			</Template>
			
			<!--
			**********************************************
			Visual- Volume peak indicator
			**********************************************
			-->
			<StatusLight>
				<TooltipId>PeakIndicator</TooltipId>
				<SizePolicy>min,min</SizePolicy>
				<PathStatusLight>led_on.svg</PathStatusLight>
				<PathBack>led_off.svg</PathBack>
				<Connection>
					<ConfigKey><Variable name="channel"/>,PeakIndicator</ConfigKey>
				</Connection>
			</StatusLight>
			
			<Template src="skin:skin_spacer_vx.xml">
				<SetVariable name="height">3</SetVariable>
			</Template>
			
			<!--
			**********************************************
			Visual - Volume level display
			TODO : sizes?
			**********************************************
			-->
			<SetVariable name="vumeter_width">15</SetVariable>
			<SetVariable name="vumeter_height">51</SetVariable>
			
			<VuMeter>
				<TooltipId>channel_VuMeter</TooltipId>
				<PathVu>vumeter_on.svg</PathVu>
				<PathBack>vumeter_off.svg</PathBack>
				
				<Horizontal>false</Horizontal>
				
				<PeakHoldSize><Variable name="vumeter_height"/></PeakHoldSize>
				<PeakHoldTime>500</PeakHoldTime>
				<PeakFallTime>50</PeakFallTime>
				<PeakFallStep>1</PeakFallStep>
				
				<Connection>
					<ConfigKey><Variable name="channel"/>,VuMeter</ConfigKey>
				</Connection>
			</VuMeter>
			
			<Template src="skin:skin_spacer_vx.xml">
				<SetVariable name="height">4</SetVariable>
			</Template>
			
			
			<WidgetGroup>
				<Layout>horizontal</Layout>
				<MinimumSize>15,15</MinimumSize>
				<MaximumSize>15,15</MaximumSize>
				<Children>
					
					<Template src="skin:skin_opener.xml">
						<SetVariable name="action">show_pregain</SetVariable>
					</Template>
					
				</Children>
			</WidgetGroup>
			
		</Children>
	</WidgetGroup>

	<Template src="skin:skin_spacer_vx.xml">
		<SetVariable name="height">0</SetVariable>
	</Template>
	
	<!--
	**********************************************
	 : Gain
	**********************************************
	-->

	<WidgetGroup>
		<Layout>vertical</Layout>
		<SizePolicy>min,min</SizePolicy>
		<MinimumSize>33,-1</MinimumSize>
		<MaximumSize>69,-1</MaximumSize>
		<Children>
			
			<Template src="skin:skin_spacer_vx.xml">
				<SetVariable name="height">16</SetVariable>
			</Template>
			
			<WidgetGroup>
				<Layout>horizontal</Layout>
				<Size>,69</Size>
				<SizePolicy>min,min</SizePolicy>
				<Children>
					
					<SetVariable name="color"><Variable name="track_light_color"/></SetVariable>
					
					<SetVariable name="width">15</SetVariable>
					<SetVariable name="height">69</SetVariable>
					<SetVariable name="offset">7</SetVariable>
					<SetVariable name="margin">5.5</SetVariable>
					
					<SetVariable name="handle_width">15</SetVariable>
					<SetVariable name="handle_height">11</SetVariable>
					
					<SetVariable name="text">gai</SetVariable>
					<SetVariable name="text_x">6</SetVariable>
					<SetVariable name="text_y">60</SetVariable>
					
					<SliderComposed>
						<TooltipId>pregain</TooltipId>
						<Slider>slider-vertical.svg</Slider>
						<Handle>handle-vertical.svg</Handle>
						<Horizontal>false</Horizontal>
						<Connection>
							<ConfigKey><Variable name="channel"/>,pregain</ConfigKey>
						</Connection>
					</SliderComposed>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">3</SetVariable>
					</Template>
					
					<SetVariable name="width">25</SetVariable>
					<SetVariable name="height">69</SetVariable>
					<SetVariable name="offset">12</SetVariable>
					<SetVariable name="margin">6.5</SetVariable>
					
					<SetVariable name="handle_width">25</SetVariable>
					<SetVariable name="handle_height">13</SetVariable>
					
					<SetVariable name="text">vol.</SetVariable>
					<SetVariable name="text_x">12</SetVariable>
					<SetVariable name="text_y">60</SetVariable>
					
					<SliderComposed>
						<TooltipId>channel_volume</TooltipId>
						<!--Style>QGroupBox { border: 1px solid red; }</Style-->
						<SizePoicy>min,min</SizePoicy>
						<Slider>slider-vertical.svg</Slider>
						<Handle>handle-vertical.svg</Handle>
						<Horizontal>false</Horizontal>
						<Connection>
							<ConfigKey><Variable name="channel"/>,volume</ConfigKey>
							<EmitOnDownPress>false</EmitOnDownPress>
						</Connection>
					</SliderComposed>
					
				</Children>
			</WidgetGroup>
				
			<Template src="skin:skin_spacer_vx.xml">
				<SetVariable name="height">4</SetVariable>
			</Template>
				
			
			<!--
			**********************************************
			Button- Prelisten / Monitoring
			**********************************************
			-->
			<SetVariable name="text">H</SetVariable>
			<PushButton>
				<TooltipId>pfl</TooltipId>
				<MinimumSize>30,20</MinimumSize>
				<MaximumSize>40,30</MaximumSize>
				<NumberStates>2</NumberStates>
				<RightClickIsPushButton>true</RightClickIsPushButton>
				<State>
					<Number>0</Number>
					<Unpressed>btn_text_little_1x1.svg</Unpressed>
					<Pressed>btn_text_little_1x1.svg</Pressed>
				</State>
				<State>
					<Number>1</Number>
					<SetVariable name="dark_color"><Variable name="track_dark_color"/></SetVariable>
					<SetVariable name="light_color"><Variable name="track_light_color"/></SetVariable>
					<SetVariable name="text_color"><Variable name="track_text_color"/></SetVariable>
					<Unpressed>btn_text_little_1x1.svg</Unpressed>
					<Pressed>btn_text_little_1x1.svg</Pressed>
				</State>
				<Connection>
					<ConfigKey><Variable name="channel"/>,pfl</ConfigKey>
				</Connection>
			</PushButton>
			
			<Template src="skin:skin_spacer_vx.xml">
				<SetVariable name="height">3</SetVariable>
			</Template>
			
		</Children>
		<Connection>
			<ConfigKey><Variable name="channel"/>,show_pregain</ConfigKey>
			<BindProperty>visible</BindProperty>
		</Connection>
	</WidgetGroup>
	
	
	<Template src="skin:skin_spacer_vx.xml">
		<SetVariable name="height">18</SetVariable>
	</Template>
	
	<!--
	**********************************************
	Column 2
	**********************************************
	-->
	
	
	<WidgetGroup>
		<Layout>vertical</Layout>
		<MinimumSize>35,-1</MinimumSize>
		<MaximumSize>35,-1</MaximumSize>
		<Children>
			
			<Template src="skin:skin_spacer_vx.xml">
				<SetVariable name="height">14</SetVariable>
			</Template>
			
			<!--
			****************************************
			 Button- Play
			****************************************
			-->
			<PushButton>
				<TooltipId>play_cue_set</TooltipId>
				<MinimumSize>20,20</MinimumSize>
				<MaximumSize>40,30</MaximumSize>
				<NumberStates>2</NumberStates>
				<RightClickIsPushButton>true</RightClickIsPushButton>
				<State>
					<Number>0</Number>
					<SetVariable name="text">P</SetVariable>
					<Unpressed>btn_text_1.3x2.svg</Unpressed>
					<Pressed>btn_text_1.3x2.svg</Pressed>
				</State>
				<State>
					<Number>1</Number>
					<SetVariable name="text">P</SetVariable>
					<SetVariable name="light_color"><Variable name="track_light_color"/></SetVariable>
					<!--SetVariable name="text_color"><Variable name="track_text_color"/></SetVariable-->
					<!--SetVariable name="text_color">#aaaaaa</SetVariable-->
					<SetVariable name="background_color">url(#grad)</SetVariable>
					<Unpressed>btn_text_1.3x2.svg</Unpressed>
					<Pressed>btn_text_1.3x2.svg</Pressed>
				</State>
				<Connection>
					<ConfigKey><Variable name="channel"/>,play</ConfigKey>
					<EmitOnDownPress>true</EmitOnDownPress>
					<ButtonState>LeftButton</ButtonState>
				</Connection>
				<Connection>
					<ConfigKey><Variable name="channel"/>,cue_set</ConfigKey>
					<EmitOnDownPress>true</EmitOnDownPress>
					<ButtonState>RightButton</ButtonState>
					<ConnectValueToWidget>false</ConnectValueToWidget>
				</Connection>
			</PushButton>
			
			<Template src="skin:skin_spacer_vx.xml">
				<SetVariable name="height">3</SetVariable>
			</Template>
			
			<!--
			**********************************************
			Button- Beat sync
			**********************************************
			-->
			<SetVariable name="text">S</SetVariable>
			<PushButton>
				<TooltipId>beatsync_beatsync_tempo</TooltipId>
				<NumberStates>2</NumberStates>
				<State>
					<Number>0</Number>
					<Unpressed>btn_text_1.3x2.svg</Unpressed>
					<Pressed>btn_text_1.3x2.svg</Pressed>
				</State>
				<State>
					<Number>1</Number>
					<SetVariable name="dark_color"><Variable name="track_dark_color"/></SetVariable>
					<SetVariable name="light_color"><Variable name="track_light_color"/></SetVariable>
					<SetVariable name="text_color"><Variable name="track_text_color"/></SetVariable>
					<Unpressed>btn_text_1.3x2.svg</Unpressed>
					<Pressed>btn_text_1.3x2.svg</Pressed>
				</State>
				<Connection>
					<ConfigKey><Variable name="channel"/>,sync_enabled</ConfigKey>
					<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
					<ButtonState>LeftButton</ButtonState>
				</Connection>
			</PushButton>
			
			<Template src="skin:skin_spacer_vx.xml">
				<SetVariable name="height">3</SetVariable>
			</Template>
			
			<WidgetGroup>
				<Layout>horizontal</Layout>
				<MinimumSize>33,15</MinimumSize>
				<MaximumSize>33,15</MaximumSize>
				<Children>
					
					<SetVariable name="text">&lt;</SetVariable>
					<PushButton>
						<TooltipId>rate_temp_up</TooltipId>
						<NumberStates>2</NumberStates>
						<State>
							<Number>0</Number>
							<Pressed>btn_text_little_1x1.svg</Pressed>
							<Unpressed>btn_text_little_1x1.svg</Unpressed>
						</State>
						<State>
							<Number>1</Number>
							<SetVariable name="dark_color"><Variable name="track_dark_color"/></SetVariable>
							<SetVariable name="light_color"><Variable name="track_light_color"/></SetVariable>
							<SetVariable name="text_color"><Variable name="track_text_color"/></SetVariable>
							<Pressed>btn_text_little_1x1.svg</Pressed>
							<Unpressed>btn_text_little_1x1.svg</Unpressed>
						</State>
						<Connection>
							<ConfigKey><Variable name="channel"/>,rate_temp_up</ConfigKey>
						</Connection>
					</PushButton>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">3</SetVariable>
					</Template>
					
					<SetVariable name="text">&gt;</SetVariable>
					<PushButton>
						<TooltipId>rate_temp_down</TooltipId>
						<NumberStates>2</NumberStates>
						<State>
							<Number>0</Number>
							<Pressed>btn_text_little_1x1.svg</Pressed>
							<Unpressed>btn_text_little_1x1.svg</Unpressed>
						</State>
						<State>
							<Number>1</Number>
							<SetVariable name="dark_color"><Variable name="track_dark_color"/></SetVariable>
							<SetVariable name="light_color"><Variable name="track_light_color"/></SetVariable>
							<SetVariable name="text_color"><Variable name="track_text_color"/></SetVariable>
							<Pressed>btn_text_little_1x1.svg</Pressed>
							<Unpressed>btn_text_little_1x1.svg</Unpressed>
						</State>
						<Connection>
							<ConfigKey><Variable name="channel"/>,rate_temp_down</ConfigKey>
						</Connection>
					</PushButton>
					
				</Children>
			</WidgetGroup>
			
			<Template src="skin:skin_spacer_vx.xml">
				<SetVariable name="height">10</SetVariable>
			</Template>
			
			
			<Template src="skin:skin_opener.xml">
				<SetVariable name="action">show_cues</SetVariable>
			</Template>
			
			
			
			
        </Children>
      </WidgetGroup>






	<!--
	**********************************************
	Column 3 
	**********************************************
	-->

	<Template src="skin:skin_spacer_vx.xml">
		<SetVariable name="height">7</SetVariable>
	</Template>
	
	<!--
	**********************************************
	Column 4
	**********************************************
	-->
	<WidgetGroup>
		<ObjectName>PositionningColumn</ObjectName>
		<Layout>horizontal</Layout>
		<SizePolicy>min,me</SizePolicy>
		<Children>
			
			<WidgetGroup>
				<Layout>vertical</Layout>
				<SizePolicy>min,me</SizePolicy>
				<MinimumSize>25,-1</MinimumSize>
				<MaximumSize>25,-1</MaximumSize>
				<Children>
					
					<SetVariable name="width">25</SetVariable>
					<SetVariable name="height">69</SetVariable>
					<SetVariable name="offset">12</SetVariable>
					<SetVariable name="margin">6.5</SetVariable>
					
					<SetVariable name="handle_width">25</SetVariable>
					<SetVariable name="handle_height">13</SetVariable>
					
					<SetVariable name="text">pitch</SetVariable>
					<SetVariable name="text_x">12.5</SetVariable>
					<SetVariable name="text_y">60</SetVariable>
					
					<SliderComposed>
						<TooltipId>pitch_rate</TooltipId>
						<Slider>slider-vertical.svg</Slider>
						<Handle>handle-vertical.svg</Handle>
						<Horizontal>false</Horizontal>
						<Alignment>center</Alignment>
						<Connection>
							<ConfigKey><Variable name="channel"/>,rate</ConfigKey>
							<EmitOnDownPress>false</EmitOnDownPress>
						</Connection>
					</SliderComposed>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">6</SetVariable>
					</Template>
					
				</Children>
			</WidgetGroup>
			
			<Template src="skin:skin_spacer_vx.xml">
				<SetVariable name="height">7</SetVariable>
			</Template>
			
			<WidgetGroup>
				<Layout>vertical</Layout>
				<SizePolicy>min,me</SizePolicy>
				<MinimumSize>24,-1</MinimumSize>
				<Children>
					
					<!--
					**********************************************
					Button- Sync Master
					**********************************************
					-->
					<SetVariable name="text">M</SetVariable>
					<PushButton>
						<ObjectName>DeckSyncMaster</ObjectName>
						<Size>40f,20f</Size>
						<NumberStates>2</NumberStates>
						<State>
							<Number>0</Number>
							<Unpressed>btn_text_little_1x1.svg</Unpressed>
							<Pressed>btn_text_little_1x1.svg</Pressed>
						</State>
						<State>
							<Number>1</Number>
							<SetVariable name="dark_color"><Variable name="track_dark_color"/></SetVariable>
							<SetVariable name="light_color"><Variable name="track_light_color"/></SetVariable>
							<SetVariable name="text_color"><Variable name="track_text_color"/></SetVariable>
							<Unpressed>btn_text_little_1x1.svg</Unpressed>
							<Pressed>btn_text_little_1x1.svg</Pressed>
						</State>
						<Connection>
							<ConfigKey><Variable name="channel"/>,sync_master</ConfigKey>
							<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
							<ButtonState>LeftButton</ButtonState>
						</Connection>
					</PushButton>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">3</SetVariable>
					</Template>
					
					<!--
					**********************************************
					Button - BPM Tap
					**********************************************
					-->
					<SetVariable name="text">T</SetVariable>
					<PushButton>
						<TooltipId>bpm_tap</TooltipId>
						<NumberStates>1</NumberStates>
						<State>
							<Number>0</Number>
							<Unpressed>btn_text_little_1x1.svg</Unpressed>
							<Pressed>btn_text_little_1x1.svg</Pressed>
						</State>
						<Connection>
							<ConfigKey><Variable name="channel"/>,bpm_tap</ConfigKey>
							<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
						</Connection>
					</PushButton>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">3</SetVariable>
					</Template>
					
					<!--
					**********************************************
					Button - Quantize
					**********************************************
					-->
					<SetVariable name="text">Q</SetVariable>
					<PushButton>
						<TooltipId>quantize</TooltipId>
						<MinimumSize>40,20</MinimumSize>
						<MaximumSize>60,40</MaximumSize>
						<NumberStates>2</NumberStates>
						<State>
							<Number>0</Number>
							<Pressed>btn_text_little_1x1.svg</Pressed>
							<Unpressed>btn_text_little_1x1.svg</Unpressed>
						</State>
						<State>
							<Number>1</Number>
							<SetVariable name="dark_color"><Variable name="track_dark_color"/></SetVariable>
							<SetVariable name="light_color"><Variable name="track_light_color"/></SetVariable>
							<SetVariable name="text_color"><Variable name="track_text_color"/></SetVariable>
							<Pressed>btn_text_little_1x1.svg</Pressed>
							<Unpressed>btn_text_little_1x1.svg</Unpressed>
						</State>
						<Connection>
							<ConfigKey><Variable name="channel"/>,quantize</ConfigKey>
							<EmitOnPressAndRelease>true</EmitOnPressAndRelease>
							<ButtonState>LeftButton</ButtonState>
						</Connection>
					</PushButton>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">3</SetVariable>
					</Template>
					
					<!--
					**********************************************
					Button- Key lock
					**********************************************
					-->
					
					<SetVariable name="text">K</SetVariable>
					<PushButton>
						<TooltipId>keylock</TooltipId>
						<NumberStates>2</NumberStates>
						<State>
							<Number>0</Number>
							<Pressed>btn_text_little_1x1.svg</Pressed>
							<Unpressed>btn_text_little_1x1.svg</Unpressed>
						</State>
						<State>
							<Number>1</Number>
							<SetVariable name="dark_color"><Variable name="track_dark_color"/></SetVariable>
							<SetVariable name="light_color"><Variable name="track_light_color"/></SetVariable>
							<SetVariable name="text_color"><Variable name="track_text_color"/></SetVariable>
							<Pressed>btn_text_little_1x1.svg</Pressed>
							<Unpressed>btn_text_little_1x1.svg</Unpressed>
						</State>
						<Connection>
							<ConfigKey><Variable name="channel"/>,keylock</ConfigKey>
						</Connection>
					</PushButton>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">6</SetVariable>
					</Template>
					
				</Children>
			</WidgetGroup>
			
			
			<!--
			**********************************************
			Button- Hotcue
			**********************************************
			-->
			<WidgetGroup>
				<Layout>vertical</Layout>
				<SizePolicy>min,me</SizePolicy>
				<MinimumSize>24,-1</MinimumSize>
				<Children>
					
					<Template src="skin:skin_btn_number.xml">
						<SetVariable name="tooltip_id">hotcue</SetVariable>
						<SetVariable name="number">1</SetVariable>
						<SetVariable name="left_control">hotcue_<Variable name="number"/>_activate</SetVariable>
						<SetVariable name="right_control">hotcue_<Variable name="number"/>_clear</SetVariable>
						<SetVariable name="display_control">hotcue_<Variable name="number"/>_enabled</SetVariable>
					</Template>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">3</SetVariable>
					</Template>
					
					<Template src="skin:skin_btn_number.xml">
						<SetVariable name="tooltip_id">hotcue</SetVariable>
						<SetVariable name="number">2</SetVariable>
						<SetVariable name="left_control">hotcue_<Variable name="number"/>_activate</SetVariable>
						<SetVariable name="right_control">hotcue_<Variable name="number"/>_clear</SetVariable>
						<SetVariable name="display_control">hotcue_<Variable name="number"/>_enabled</SetVariable>
					</Template>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">3</SetVariable>
					</Template>
					
					<Template src="skin:skin_btn_number.xml">
						<SetVariable name="tooltip_id">hotcue</SetVariable>
						<SetVariable name="number">3</SetVariable>
						<SetVariable name="left_control">hotcue_<Variable name="number"/>_activate</SetVariable>
						<SetVariable name="right_control">hotcue_<Variable name="number"/>_clear</SetVariable>
						<SetVariable name="display_control">hotcue_<Variable name="number"/>_enabled</SetVariable>
					</Template>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">3</SetVariable>
					</Template>
					
					<Template src="skin:skin_btn_number.xml">
						<SetVariable name="tooltip_id">hotcue</SetVariable>
						<SetVariable name="number">4</SetVariable>
						<SetVariable name="left_control">hotcue_<Variable name="number"/>_activate</SetVariable>
						<SetVariable name="right_control">hotcue_<Variable name="number"/>_clear</SetVariable>
						<SetVariable name="display_control">hotcue_<Variable name="number"/>_enabled</SetVariable>
					</Template>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">6</SetVariable>
					</Template>
					
				</Children>
			</WidgetGroup>
			
			
			<!--
			**********************************************
			Beatloop
			**********************************************
			-->
			<WidgetGroup>
				<ObjectName>BeatLoopLowerRow</ObjectName>
				<Layout>vertical</Layout>
				<SizePolicy>min,me</SizePolicy>
				<MinimumSize>24,-1</MinimumSize>
				<Children>
					
					<Template src="skin:skin_btn_number.xml">
						<SetVariable name="tooltip_id">beatloop</SetVariable>
						<SetVariable name="number">4</SetVariable>
						<SetVariable name="left_control">beatloop_<Variable name="number"/>_toggle</SetVariable>
						<SetVariable name="right_control">beatlooproll_<Variable name="number"/>_activate</SetVariable>
						<SetVariable name="display_control">beatloop_<Variable name="number"/>_enabled</SetVariable>
					</Template>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">3</SetVariable>
					</Template>
					
					<Template src="skin:skin_btn_number.xml">
						<SetVariable name="tooltip_id">beatloop</SetVariable>
						<SetVariable name="number">8</SetVariable>
						<SetVariable name="left_control">beatloop_<Variable name="number"/>_toggle</SetVariable>
						<SetVariable name="right_control">beatlooproll_<Variable name="number"/>_activate</SetVariable>
						<SetVariable name="display_control">beatloop_<Variable name="number"/>_enabled</SetVariable>
					</Template>
					
					<Template src="skin:skin_spacer_vx.xml">
						<SetVariable name="height">3</SetVariable>
					</Template>
					
					<Template src="skin:skin_btn_number.xml">
